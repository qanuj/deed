@model Deed.Data.Legacy.Story

@{
    ViewBag.Title = "How It Works";
}
@Styles.Render("~/WhatwedoStyles/css")
<style>
    body {
        min-height: 960px;
        -webkit-box: inset 0 0 0 5px #f2f2f2,inset 0 0 0 10px #90c0aa,inset 0 0 0 15px #f2f2f2;
        -mox-box: inset 0 0 0 5px #f2f2f2,inset 0 0 0 10px #90c0aa,inset 0 0 0 15px #f2f2f2;
        margin: 0;
    }
</style>

<div class="page-head" style="background-repeat: no-repeat; background-position: center top; background-image: url('/Content/images/SliderImg/13772.jpg'); background-size: cover; ">
    <div class="container">
        <div class="wrap clearfix">
            <h1 class="page-title"><span>How It Works</span></h1>
            <p></p>
        </div>
    </div>
</div>
<div class="container contents single login-register" style="margin-top:3px">
    <div class="row">
        <div class="span7 main-wrap">
            <section id="loginForm">
                <h3><span>How It Works</span></h3>
                <div class="main" style="align-self:center;">
                    <div class="inner-wrapper">
                        <figure>
                            <img src="http://deedaday.in/img/upload/story/@Model.image" />
                        </figure>
                        <article class="post-25 post type-post status-publish format-image has-post-thumbnail hentry category-sales tag-custom tag-interior">
                            <h3>@Model.title</h3><br />

                            
                            <blockquote>
                                <p>@Model.content</p>
                            </blockquote>
                            
                        </article>
                    </div>
                </div>
            </section>
        </div>
        <div class="span4 main-wrap pull-right">
            <section id="loginForm">
                @*<h3><span>How It Works</span></h3>*@
                <div class="main" style="align-self:center;">
                    <div class="inner-wrapperr" style="margin-top: 30px;">
                        <article class="post-25 post type-post status-publish format-image has-post-thumbnail hentry category-sales tag-custom tag-interior">
                            @*<p style="font-size:150%;">Coffee a Day cost more than a Pound , Deed a Day cost less than a Coffee a day, 1 pound *365 days can help educating a child <br>(schooling +uniform +books ).</p>*@
                            <div class="pull-right"><figure><img src="~/Content/images/old_edit_undo1.png" alt=""></figure></div>
                            <figure style="margin-bottom:15px;"><p style="font-size:150%;"> &#163; 1.5</p><img src="data:image/jpeg;base64," alt=""></figure>

                            <blockquote>
                                <p style="font-size:130%;">
                                    Deed a Day Costs Less Than
                                    Coffee a Day
                                </p>
                            </blockquote>


                            <p style="font-size:150%;"> &#163; 1 X 365 Days</p>
                            <p class="success">That adds up to &#163; 365 a year</p>
                            <figure><img src="~/Content/images/old_edit_undo2.png" alt=""></figure>
                            <figure><img src="http://deedaday.in//images/school.png" alt=""></figure>
                            <blockquote>
                                <p> Educating a child(Schooling+<br>Uniform+<br>Books)</p>
                            </blockquote>
                        </article>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>







